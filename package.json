{
  "name": "alamode",
  "version": "2.4.0",
  "description": "A Regex-Based Transpiler Of Source Code To Allow Writing Import And Export Statements And JSX With 0 Dependencies.",
  "main": "compile/index.js",
  "scripts": {
    "t": "zoroaster -a",
    "test": "yarn t test/spec test/mask",
    "spec": "yarn t test/spec",
    "mask": "yarn t test/mask",
    "test-compile": "yarn t test/mask/bin.js",
    "lint": "eslint .",
    "dev": "NODE_DEBUG=alamode node src/bin",
    "doc": "NODE_DEBUG=doc doc -o README.md",
    "e": "alanode",
    "example/": "yarn e example/example.js",
    "build": "yarn-s b doc",
    "wiki": "NODE_DEBUG=doc doc wiki -W ../alamode.wiki",
    "d": "yarn-s d1 d2",
    "d1": "typal types/index.js compile/index.js -u",
    "d2": "typal types/externs.js",
    "compile": "depack-dev src/bin/alamode -c -a -p -o compile/bin --externs types/externs.js -s -S -O 2018",
    "compile-lib": "depack-dev src/depack -c -a -p -o compile -s -S",
    "alamode": "compile/bin/alamode.js",
    "args": "argufy -o src/bin/get-args.js",
    "b": "compile/bin/alamode.js src/bin/alanode.js -o build -s"
  },
  "bin": {
    "alamode": "compile/bin/alamode.js",
    "alamode-dev": "src/bin/index.js",
    "alanode": "build/alanode.js"
  },
  "files": [
    "build",
    "src/bin/index.js",
    "compile",
    "types"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/a-la/alamode.git"
  },
  "keywords": [
    "alamode",
    "transpiler",
    "transpile",
    "compiler",
    "compile",
    "import",
    "export",
    "exports",
    "module",
    "default",
    "require",
    "a-la",
    "regular expression",
    "es6 modules",
    "es6",
    "modules",
    "regex",
    "regexp",
    "babel",
    "ECMAScript",
    "ecma",
    "jsx",
    "components"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/a-la/alamode/issues"
  },
  "homepage": "https://github.com/a-la/alamode#readme",
  "devDependencies": {
    "@a-la/export": "^1.5.0",
    "@a-la/fixture-alamode": "1.1.3",
    "@a-la/fixture-babel": "1.2.2",
    "@a-la/import": "^1.10.1",
    "@a-la/jsx": "^1.6.0",
    "@a-la/markers": "^1.3.0",
    "@a-la/source-map-generator": "^1.0.0",
    "@artdeco/pirates": "^1.0.0",
    "@wrote/clone": "^1.2.1",
    "@wrote/wrote": "^1.2.3",
    "argufy": "^1.7.1",
    "catchment": "^3.3.0",
    "documentary": "^1.32.1",
    "erte": "^1.2.0",
    "eslint-config-artdeco": "1.0.1",
    "preact": "8.5.2",
    "restream": "^3.7.4",
    "spawncommand": "^2.2.0",
    "temp-context": "^2.1.3",
    "typal": "^1.22.1",
    "usually": "^1.2.0",
    "which-stream": "^1.2.1",
    "yarn-s": "1.1.0",
    "zoroaster": "^4.1.1-alpha"
  }
}
